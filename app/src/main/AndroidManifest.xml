<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.s15720PAMO">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="com.example.s15720PAMO.BMI.BmiCalculator">
        </activity>
        <activity
                android:name="com.example.s15720PAMO.MainActivity"
                android:label="@string/app_name_main"
                android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.example.s15720PAMO.Quiz.QuizActivity"
                  android:label="@string/Quiz">
        </activity>
        <activity android:name="com.example.s15720PAMO.Chart.ChartMenuActivity"
            android:label="@string/chart">
        </activity>
        <activity android:name="com.example.s15720PAMO.Chart.Charts.InfectedChartActivity"
            android:label="@string/chart">
        </activity>
        <activity android:name="com.example.s15720PAMO.Chart.Charts.RecoveriesChartActivity"
            android:label="@string/chart">
        </activity>
        <activity android:name="com.example.s15720PAMO.Chart.Charts.DeathChartActivity"
            android:label="@string/chart">
        </activity>
    </application>

</manifest>
